<html>

<head>
  <title>
    <%= title %>
  </title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <h1>
    <%= title %>
  </h1>
  <p><b>Welcome to <%= sub %></b>
  </p>
  <table>

    <% names.forEach((name)=> { %>
      <tr>
        <td>
          <%= name.id %>
        </td>
        <td>
          <%= name.term %>
        </td>
        <td>
          <%= name.description %>
        </td>
        <td>
          <%= name. reference %>
        </td>
      </tr>
      <% }) %>
  </table>
</body>

</html>